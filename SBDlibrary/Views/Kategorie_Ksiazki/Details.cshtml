@{
        ViewData["Title"] = "Egzemplarze";
    }
    
    <h1>Details</h1>
    <h1>ksiazki</h1>

    @model SBDlibrary.Models.KatModel
    <table class="table">
        <thead>
            <tr>
                <dd class="col-sm-10">
                    @Html.DisplayFor(model => model.tytul)
                </dd>
                <th>
                    @Html.DisplayNameFor(model => model.kategorie)
                </th>

            </tr>
        </thead>   
        <tbody>
            @foreach (var item in Model.kategorie)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item)
                    </td>

                </tr>
            }
        </tbody>
    </table>
