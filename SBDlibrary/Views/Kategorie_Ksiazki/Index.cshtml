<link href="https://fonts.googleapis.com/css?family=Lato&display=swap" rel="stylesheet">
<style>
    h1 {
        font-family: 'Lato', monospace;
    }
</style>
@model IEnumerable<SBDlibrary.Models.Kategorie_Ksiazki>

@{
    ViewData["Title"] = "Kategoria";
}


<h1>ksiazki</h1>


<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.id_ksiazki)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.id_kategorii)
            </th>

        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.id_ksiazki)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.id_kategorii)
            </td>

        </tr>
        }
    </tbody>
</table>